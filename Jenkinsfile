pipeline {
  agent any
  
  stages {
    stage('Build') {
      steps {
        echo 'Hello World'
      }
    }
    stage('Login') {
      steps {
        echo 'Hello World'
      }
    }
    stage('Push') {
      steps {
       echo 'Hello World'
      }
    }
  }
  post {
    always {
      echo 'Hello World'
    }
  }
}
